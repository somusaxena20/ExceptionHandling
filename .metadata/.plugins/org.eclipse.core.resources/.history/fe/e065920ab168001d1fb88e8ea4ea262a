package com.somu.top10_exception;

public class Top10Exception {

	public static void main(String[] args) {
		
//		ArrayOutOfBoundException -> when you try to access out of array range 
		
//		int []a = new int[10];
//		System.out.println(a[10]);
		
//		NullPointerException -> when you try to perform any operation on NULL it cause
		
//		String str = null;
//		System.out.println(str.length());
		
//		ClassCastException
		
		Object obj = new Object("SOMU");
	}

}
